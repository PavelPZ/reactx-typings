{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",

    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "noImplicitAny": false,
    "jsx": "react",
    "lib": [
      "dom",
      "es2015",
      "esnext.asynciterable"
    ]
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules",
  ]
}
